{
    "data": {
        "motion_loader": {
            "_target_": "src.data.motion.AMASSMotionLoader",
            "base_dir": "datasets/motions/guoh3dfeats",
            "normalizer": {
                "_target_": "TMR.src.data.motion.Normalizer",
                "base_dir": "TMR/stats/humanml3d_kitml/guoh3dfeats",
                "eps": 1e-12
            },
            "fps": 20.0,
            "nfeats": 263
        },
        "_target_": "TMR.src.data.augmented_text_motion.AugmentedTextMotionDataset",
        "path": "datasets/annotations/humanml3d_kitml",
        "text_to_token_emb": {
            "_target_": "TMR.src.data.text.TokenEmbeddings",
            "path": "datasets/annotations/humanml3d_kitml",
            "modelname": "distilbert-base-uncased",
            "modelpath": null,
            "preload": true,
            "name": "token_embeddings_all"
        },
        "text_to_sent_emb": {
            "_target_": "TMR.src.data.text.SentenceEmbeddings",
            "path": "datasets/annotations/humanml3d_kitml",
            "modelname": "sentence-transformers/all-mpnet-base-v2",
            "modelpath": null,
            "preload": true,
            "name": "sent_embeddings_all"
        },
        "preload": true,
        "paraphrase_filename": "annotations_paraphrases.json",
        "summary_filename": "annotations_actions.json",
        "paraphrase_prob": 0.2,
        "summary_prob": 0.1,
        "averaging_prob": 0.3,
        "text_sampling_nbr": null
    },
    "model": {
        "_target_": "TMR.src.model.tmr_text_averaging.TMRTextAveraging",
        "motion_encoder": {
            "_target_": "TMR.src.model.ACTORStyleEncoder",
            "nfeats": 263,
            "vae": true,
            "latent_dim": 256,
            "ff_size": 1024,
            "num_layers": 6,
            "num_heads": 4,
            "dropout": 0.1,
            "activation": "gelu"
        },
        "text_encoder": {
            "_target_": "TMR.src.model.ACTORStyleEncoder",
            "nfeats": 768,
            "vae": true,
            "latent_dim": 256,
            "ff_size": 1024,
            "num_layers": 6,
            "num_heads": 4,
            "dropout": 0.1,
            "activation": "gelu"
        },
        "motion_decoder": {
            "_target_": "TMR.src.model.ACTORStyleDecoder",
            "nfeats": 263,
            "latent_dim": 256,
            "ff_size": 1024,
            "num_layers": 6,
            "num_heads": 4,
            "dropout": 0.1,
            "activation": "gelu"
        },
        "vae": true,
        "lmd": {
            "recons": 1.0,
            "latent": 1e-05,
            "kl": 1e-05,
            "contrastive": 0.1
        },
        "lr": 0.0001,
        "threshold_selfsim_metrics": 0.95,
        "contrastive_loss": {
            "_target_": "TMR.src.model.losses.HN_InfoNCE_with_filtering",
            "temperature": 0.1,
            "threshold_selfsim": 0.8,
            "alpha": 0.999,
            "beta": 0.5
        }
    },
    "trainer": {
        "_target_": "pytorch_lightning.Trainer",
        "max_epochs": 500,
        "log_every_n_steps": 50,
        "num_sanity_val_steps": 0,
        "check_val_every_n_epoch": 1,
        "accelerator": "gpu",
        "devices": 1,
        "callbacks": [
            {
                "_target_": "pytorch_lightning.callbacks.ModelCheckpoint",
                "filename": "latest-{epoch}",
                "every_n_epochs": 1,
                "save_top_k": 1,
                "save_last": true
            },
            {
                "_target_": "pytorch_lightning.callbacks.ModelCheckpoint",
                "filename": "latest-{epoch}",
                "monitor": "step",
                "mode": "max",
                "every_n_epochs": 100,
                "save_top_k": -1,
                "save_last": false
            },
            {
                "_target_": "src.callback.progress.ProgressLogger",
                "precision": 3
            },
            {
                "_target_": "src.callback.tqdmbar.TQDMProgressBar"
            }
        ],
        "logger": {
            "_target_": "src.logger.csv.CSVLogger",
            "save_dir": "outputs/tmr_text_averaging_hn_humanml3d_kitml_augmented_guoh3dfeats",
            "name": "logs"
        }
    },
    "run_dir": "outputs/tmr_text_averaging_hn_humanml3d_kitml_augmented_guoh3dfeats",
    "seed": 1234,
    "logger_level": "INFO",
    "ckpt": "last",
    "resume_dir": null,
    "ckpt_path": null,
    "dataloader": {
        "_target_": "torch.utils.data.DataLoader",
        "batch_size": 32,
        "num_workers": 8
    }
}